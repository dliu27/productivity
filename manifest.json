{
	"manifest_version": 2,
	"name": "Productivity++",
	"version": "1.1",
	"description": "by dliu27.",
	"permissions": [
		"storage",
		"notifications"
	],
	"content_security_policy": "script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs='",
	"chrome_url_overrides" : {
    	"newtab": "newTab.html"
	},

	"browser_action": {
		"default_title": "Productivity++",
		"default_icon": "128.png"
	},

	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}